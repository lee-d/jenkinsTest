pipeline {
    agent any
    triggers {
        pollSCM 'H/5 * * * *'
        cron 'H H(0-5) * * *'
    }
    environment {
        SERVICE = 'foo'
    }
    stages {
        stage('Maven Build') {
            steps {
                sh "mvn -B clean verify -DskipTests"
                dependencyCheckPublisher pattern: 'target/dependency-check-report.*'

                script {
                    myDataMap = [:]
                    myDataMap['myKey'] = 'aksdfjklasjfdlkakldfjlkajsdk;fjlkaf awsefasdfaj askjfalksjdf kjalkfjd '
                }
                step([$class: 'InfluxDbPublisher', customData: null, customPrefix: 'myData', target: 'grafana'])
            }
        }
    }
}
